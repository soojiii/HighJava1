package vo.Campaign;

public class CampaignVo {
	
	private int cam_no;			//캠페인 게시판 번호
	private String admin_id;	//관리자아이디
	private String cam_title;	//캠페인제목
	private String cam_content;	//캠페인 내용
	private String cam_date;	//캠페인 날짜
	private int cam_hits;		//캠페인 조회수
	private String cam_del;		//캠페인 삭제

	public int getCam_no() {
		return cam_no;
	}
	public void setCam_no(int cam_no) {
		this.cam_no = cam_no;
	}
	public String getAdmin_id() {
		return admin_id;
	}
	public void setAdmin_id(String admin_id) {
		this.admin_id = admin_id;
	}
	public String getCam_title() {
		return cam_title;
	}
	public void setCam_title(String cam_title) {
		this.cam_title = cam_title;
	}
	public String getCam_content() {
		return cam_content;
	}
	public void setCam_content(String cam_content) {
		this.cam_content = cam_content;
	}
	public String getCam_date() {
		return cam_date;
	}
	public void setCam_date(String cam_date) {
		this.cam_date = cam_date;
	}
	public int getCam_hits() {
		return cam_hits;
	}
	public void setCam_hits(int cam_hits) {
		this.cam_hits = cam_hits;
	}
	public String getCam_del() {
		return cam_del;
	}
	public void setCam_del(String cam_del) {
		this.cam_del = cam_del;
	}
	
}
